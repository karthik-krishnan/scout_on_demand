<h1>New user</h1>
<table>
<% form_for(@user) do |f| %>
  <tr><td><label for='user_user_id'>User Id:</label</td><td><%= f.text_field :user_id %></td>
  <tr><td><label for='user_user_name'>User Name:</label</td><td><%= f.text_field :user_name %></td>
  <%= f.error_messages %>
  <tr><td><%= f.submit "Create" %></td>
<% end %>
</table>
<%= link_to 'Back', users_path %>
