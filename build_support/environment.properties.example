application.base_url=http://forum:3080
application.internal_base_url=http://localhost:3080


sso_server.base_url=http://forum:3080

# If application should talk to SSO Server deployed in the same instance, then comment out the below property.
sso_server.internal_base_url=http://localhost:3080


