# -------------------
ops.runtimes.min=1
ops.runtimes.max=1
ops.runtimes.sharable=true
mc.runtimes.min=1
mc.runtimes.max=1
mc.runtimes.sharable=true
sso_admin.runtimes.min=1
sso_admin.runtimes.max=1
sso_admin.runtimes.sharable=true
# -------------------

# Diagnostic Log info
# -------------------
diagnostic_log_dir=/tmp

